@model IEnumerable<n01625423_cumulative_project_1.Models.Course>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>This page should be showing information about all classes</p>

<div id="containerlist">
    <h2>List of Classes</h2>
    @foreach (var classes in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Class/Show/@classes.ClassID">@classes.ClassCode @classes.ClassName</a></div>
        </div>
    }
</div>